<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
</head>
<body>
    <h1>All Users</h1>
    <ul>
        <% users.forEach(function(user) { %>
            <li>
                <a href="/users/<%= user._id %>"><%= user.username %></a>
            </li>
        <% }); %>
    </ul>
</body>
</html>
